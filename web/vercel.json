{
  "crons": [
    {
      "path": "/api/cron/sync-stats",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/sync-twitter",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/monitor-hashtags",
      "schedule": "0 2 * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
